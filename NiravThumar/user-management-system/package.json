{
  "name": "user-management-system",
  "version": "1.0.0",
  "description": "A secure user management system with role-based access control",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "seed": "node scripts/seedRoles.js",
    "clear-users": "node scripts/clearUserData.js",
    "make-admin": "node scripts/makeAdmin.js",
    "prepare": "husky install"
  },
  "keywords": [
    "user-management",
    "authentication",
    "authorization",
    "rbac",
    "express",
    "mongodb"
  ],
  "author": "Nirav Thumar",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "bcrypt": "^6.0.0",
    "eslint": "^8.36.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
